<Window x:Class="SpeedReading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" WindowStartupLocation="Manual" Left="600" Top="100" >
    <Grid Name="theGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        
        <Button Name="cmdStart" Grid.Row="1" Content="start" Click="cmdStart_Click"/>
    </Grid>
</Window>
